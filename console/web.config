<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Editar notícia">
                    <match url="^noticia-editar/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="noticia-editar.php?id_noticia={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar emblema">
                    <match url="^emblema-editar/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="emblema-editar.php?codigo={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Contas por IP">
                    <match url="^mod-contasip/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="mod-contasip.php?ip={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar Usuário">
                    <match url="^usuarios-editar/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="usuarios-editar?username={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Emblemas usuário">
                    <match url="^usuarios-emblemas/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="usuarios-emblemas?user_id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Quartos usuário">
                    <match url="^usuarios-quartos/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="usuarios-quartos?user_id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Inventário usuário">
                    <match url="^usuarios-inventario/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="usuarios-inventario?user_id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Ltds usuário">
                    <match url="^usuarios-ltds/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="usuarios-ltds?user_id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Grupos usuário">
                    <match url="^usuarios-grupos/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="usuarios-grupos?user_id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Items usuário">
                    <match url="^usuarios-items/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="usuarios-items?user_id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar Permissão">
                    <match url="^gerenciamento-editarpermissao/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="gerenciamento-editarpermissao?id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar Permissão Painel">
                    <match url="^gerenciamento-editarpermissaopainel/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="gerenciamento-editarpermissaopainel?id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Catálogo Items">
                    <match url="^gerenciamento-catalogo-items/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="gerenciamento-catalogo-items?id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar Item">
                    <match url="^gerenciamento-catalogo-editaritem/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="gerenciamento-catalogo-editaritem?id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar Pagina">
                    <match url="^gerenciamento-catalogo-editarpagina/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="gerenciamento-catalogo-editarpagina?id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar Texto">
                    <match url="^gerenciamento-editartexto/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="gerenciamento-editartexto?chave={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Editar VIP">
                    <match url="^vips-usuarios-editar/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="vips-usuarios-editar?id={R:1}" appendQueryString="false" />
                </rule>
                <rule name="Imported Rule 11" stopProcessing="true">
                    <match url="^([^\.]+)$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="{R:1}.php" />
                </rule>
            </rules>
        </rewrite>
        <handlers>
            <clear />
            <add name="php-7.4.13" path="*.php" verb="GET,HEAD,POST" modules="FastCgiModule" scriptProcessor="C:\Program Files\php\v7.4\v7.4\v7.4\php-cgi.exe" resourceType="Either" requireAccess="Script" />
            <add name="php-8.1.9" path="*.php" verb="GET,HEAD,POST" modules="FastCgiModule" scriptProcessor="C:\Program Files\php\php-8.1.9\php-cgi.exe" resourceType="Either" requireAccess="Script" />
            <add name="ASPClassic" path="*.asp" verb="GET,HEAD,POST" modules="IsapiModule" scriptProcessor="%windir%\system32\inetsrv\asp.dll" resourceType="File" />
            <add name="SecurityCertificate" path="*.cer" verb="GET,HEAD,POST" modules="IsapiModule" scriptProcessor="%windir%\system32\inetsrv\asp.dll" resourceType="File" />
            <add name="ISAPI-dll" path="*.dll" verb="*" modules="IsapiModule" resourceType="File" requireAccess="Execute" allowPathInfo="true" />
            <add name="AXD-ISAPI-4.0_64bit" path="*.axd" verb="GET,HEAD,POST,DEBUG" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework64\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness64" responseBufferLimit="0" />
            <add name="PageHandlerFactory-ISAPI-4.0_64bit" path="*.aspx" verb="GET,HEAD,POST,DEBUG" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework64\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness64" responseBufferLimit="0" />
